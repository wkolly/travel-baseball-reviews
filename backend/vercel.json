{
  "version": 2,
  "builds": [
    {
      "src": "api/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/admin/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/login(.*)",
      "dest": "api/login.ts"
    },
    {
      "src": "/api/register(.*)", 
      "dest": "api/register.ts"
    },
    {
      "src": "/login(.*)",
      "dest": "api/login.ts"
    },
    {
      "src": "/register(.*)", 
      "dest": "api/register.ts"
    },
    {
      "src": "/signup(.*)", 
      "dest": "api/signup.ts"
    },
    {
      "src": "/api/signup(.*)", 
      "dest": "api/signup.ts"
    },
    {
      "src": "/api/admin/stats(.*)",
      "dest": "api/admin/stats.ts"
    },
    {
      "src": "/api/admin/users(.*)",
      "dest": "api/admin/users.ts"
    },
    {
      "src": "/api/admin/teams(.*)",
      "dest": "api/admin/teams.ts"
    },
    {
      "src": "/api/admin/tournaments(.*)",
      "dest": "api/admin/tournaments.ts"
    },
    {
      "src": "/api/admin/reviews(.*)",
      "dest": "api/admin/reviews.ts"
    },
    {
      "src": "/api/admin/pending-teams(.*)",
      "dest": "api/admin/pending-teams.ts"
    },
    {
      "src": "/teams(.*)",
      "dest": "api/teams.ts"
    },
    {
      "src": "/tournaments(.*)",
      "dest": "api/tournaments-safe.ts"
    },
    {
      "src": "/reviews(.*)",
      "dest": "api/reviews.ts"
    },
    {
      "src": "/tournament-reviews(.*)",
      "dest": "api/tournament-reviews.ts"
    },
    {
      "src": "/team-reviews(.*)",
      "dest": "api/team-reviews.ts"
    },
    {
      "src": "/api/auth(.*)",
      "dest": "api/auth.ts"
    },
    {
      "src": "/api/teams(.*)",
      "dest": "api/teams.ts"
    },
    {
      "src": "/api/tournaments(.*)",
      "dest": "api/tournaments-safe.ts"
    },
    {
      "src": "/api/reviews(.*)",
      "dest": "api/reviews.ts"
    },
    {
      "src": "/api/tournament-reviews(.*)",
      "dest": "api/tournament-reviews.ts"
    },
    {
      "src": "/api/team-reviews(.*)",
      "dest": "api/team-reviews.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}