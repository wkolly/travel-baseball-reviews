{
  "version": 2,
  "builds": [
    {
      "src": "api/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/login(.*)",
      "dest": "api/auth.ts"
    },
    {
      "src": "/api/register(.*)", 
      "dest": "api/auth.ts"
    },
    {
      "src": "/login(.*)",
      "dest": "api/auth.ts"
    },
    {
      "src": "/register(.*)", 
      "dest": "api/auth.ts"
