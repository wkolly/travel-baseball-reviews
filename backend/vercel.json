    },
    {
      "src": "/api/auth(.*)",
      "dest": "api/auth.ts"
    },
    {
      "src": "/api/admin(.*)",
      "dest": "api/admin.ts"
    },
    {
      "src": "/teams(.*)",
      "dest": "api/teams.ts"
    },
    {
      "src": "/tournaments(.*)",
      "dest": "api/tournaments.ts"
    },
    {
